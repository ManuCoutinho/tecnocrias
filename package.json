{
  "name": "tecnocrias",
  "private": true,
  "version": "1.0.0",
  "license": "MIT",
  "description": "Tecnocrias group portal",
  "bugs": {
    "url": "https://github.com/ManuCoutinho/tecnocrias"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ManuCoutinho/tecnocrias"
  },
  "engines": {
    "node": ">=18.16.0",
    "npm": ">=9.5.1"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "eslint --ext .ts,.tsx src",
    "lint:css": "stylelint \"src/**/*.scss\"",
    "test": "vitest run",
    "test:watch": "vitest",
    "coverage": "vitest --coverage",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint src --fix"
    ],
    "*.{json,md, yaml}": [
      "prettier --write"
    ],
    "*.css": [
      "stylelint --fix './src/**/*.scss'"
    ]
  },
  "dependencies": {
    "lucide-react": "^0.261.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^17.6.6",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@vitejs/plugin-react": "^4.0.3",
    "@vitest/coverage-v8": "^0.33.0",
    "eslint": "^8.45.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.3",
    "identity-proxy": "^0.0.1",
    "jsdom": "^22.1.0",
    "lint-staged": "^13.2.3",
    "prettier": "^3.0.0",
    "sass": "^1.63.6",
    "stylelint-config-prettier": "^9.0.5",
    "stylelint-config-recess-order": "^3.0.0",
    "vite": "^4.4.4",
    "vitest": "^0.33.0"
  },
  "keywords": [
    "vite",
    "react",
    "tecnocrias",
    "open source",
    "sass"
  ]
}
